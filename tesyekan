local function safeSetClipboard(value)
    if not setclipboard then
        WindUI:Notify({
            Title = "Clipboard Error",
            Content = "Clipboard function is not available in this environment.",
            Icon = "alert-triangle"
        })
        return false
    end
    if value == nil or type(value) ~= "string" or value == "" then
        WindUI:Notify({
            Title = "Clipboard Error",
            Content = "Invalid input: Expected a non-empty string.",
            Icon = "alert-triangle"
        })
        return false
    end
    local success, err = pcall(function()
        setclipboard(value)
    end)
    if success then
        return true
    else
        WindUI:Notify({
            Title = "Clipboard Error",
            Content = "Failed to copy to clipboard: " .. tostring(err or "Unknown error"),
            Icon = "alert-triangle"
        })
        return false
    end
end

InfoTab:Button({
    Title = "Join Our Discord",
    Desc = "Click to copy our Discord link.",
    Callback = function()
        if safeSetClipboard("https://discord.gg/kWC8Nn7kne") then
            WindUI:Notify({
                Title = "Discord",
                Content = "Link Discord berhasil disalin ke clipboard!",
                Duration = 5
            })
        end
    end
})

